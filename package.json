{
    "name": "task-commands",
    "displayName": "Task Commands",
    "description": "Assign a command to each task in tasks.json, so that shortcuts can be assigned to tasks.",
    "version": "0.2.0",
    "publisher": "sterin",
    "repository": "https://github.com/sterin/vscode-task-commands",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "workspaceContains:.vscode/tasks.json"
    ],
    "main": "./out/src/extension",
    "contributes": {},
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}